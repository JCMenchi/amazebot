{
  "name": "amazebot-ui",
  "version": "0.2.1",
  "description": "AmazeBOT UI",
  "homepage": "/amazeui/",
  "scripts": {
    "start:dev": "webpack serve --host 0.0.0.0 --mode development",
    "start": "webpack serve --host 0.0.0.0 --mode production",
    "build:dist": "webpack --mode production",
    "build": "npm i --no-optional --no-shrinkwrap --no-package-lock && npm run build:dist",
    "doc": "jsdoc -R README.md -P package.json -d doc -r -c jsdoc.config.json frontend",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "coverage": "jest --passWithNoTests --coverage",
    "build:analyze": "npx webpack --profile --mode production --json > stats.json && npx webpack-bundle-analyzer stats.json"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/react": "^12.1.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.4.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "3.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-html-reporter": "^3.4.1",
    "jsdoc": "3.6.7",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.0",
    "webpack": "^5.59.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@fontsource/roboto": "^4.5.1",
    "@monaco-editor/react": "^4.3.1",
    "@mui/icons-material": "^5.0.4",
    "@mui/material": "^5.0.4",
    "@react-keycloak/web": "^3.4.0",
    "axios": "^0.23.0",
    "formik": "^2.2.9",
    "formik-material-ui": "^3.0.1",
    "i18next": "^21.3.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "loglevel": "^1.7.1",
    "material-ui-dropzone": "^3.5.0",
    "react": "^17.0.2",
    "react-country-flag": "^2.3.1",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.12.0",
    "react-router-dom": "^5.3.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "jest": {
    "globals": {
      "__JEST__": true
    },
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy",
      "fontsource-roboto$": "identity-obj-proxy",
      "\\-worker.js$": "<rootDir>/__mocks__/webpack-worker.js",
      "jspdf.*\\.js": "<rootDir>/__mocks__/webpack-worker.js"
    },
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Report"
        }
      ]
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "html",
      "text",
      "text-summary"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "compat"
    ],
    "rules": {
      "compat/compat": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
